<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HomeTabView">
        <attr name="icon_size" format="dimension"></attr>
        <attr name="text_size" format="dimension"></attr>
        <attr name="text_select_color" format="color"></attr>
        <attr name="text_normal_color" format="color"></attr>
        <attr name="item_padding" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="monindicator">
        <attr name="circleRadius" format="dimension" />
        <attr name="circleSpacing" format="dimension" />
        <attr name="cycle" format="integer" />
    </declare-styleable>

    <declare-styleable name="ShowPercentView">
        <attr name="percent" format="integer"></attr>
        <attr name="allLineColor" format="color" />
        <attr name="percentLineColorLow" format="color" />
        <attr name="percentLineColorHight" format="color" />
    </declare-styleable>
</resources>
